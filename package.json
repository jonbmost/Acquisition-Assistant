{
  "name": "acquisition-assistant",
  "version": "1.0.0",
  "description": "An intelligent procurement assistant to support federal acquisition teams",
  "type": "module",
  "engines": {
    "node": ">=24.0.0 <25"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:mcp",
    "build:mcp": "tsc mcp-server.ts --outDir dist --module nodenext --moduleResolution nodenext --target es2022 --skipLibCheck",
    "mcp": "node dist/mcp-server.js",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.24.3",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.4",
    "openai": "^6.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "gh-pages": "^6.1.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  }
}
